{
	"private": true,
	"name": "nuro.dev",
	"version": "3.1.1",
	"description": "Personal Website for nuro.dev",
	"repository": "https://github.com/nurodev/nuro.dev.git",
	"author": "nurodev <me@nuro.dev>",
	"license": "MIT",
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"plugins": [
			"prettier-plugin-tailwindcss"
		],
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
		"lint": "next lint",
		"postbuild": "next-sitemap --config sitemap.config.mjs",
		"start": "next start",
		"type-check": "tsc"
	},
	"dependencies": {
		"@iconify/react": "^4.0.0",
		"@motionone/react": "^10.14.3",
		"@next/font": "^13.0.2",
		"@types/node": "18.11.9",
		"@types/react": "18.0.24",
		"@types/react-dom": "18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"@vercel/analytics": "^0.1.3",
		"clsx": "^1.2.1",
		"contentlayer": "^0.2.9",
		"cva": "npm:class-variance-authority@0.2.4",
		"date-fns": "^2.29.3",
		"envsafe": "^2.0.3",
		"eslint": "8.26.0",
		"eslint-config-next": "13.0.1",
		"motion": "^10.14.3",
		"next": "^13.0.4",
		"next-contentlayer": "^0.2.9",
		"nprogress": "^0.2.0",
		"ogl-typescript": "^0.1.40",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-use": "^17.4.0",
		"react-use-lanyard": "^0.1.4",
		"typescript": "4.8.4"
	},
	"devDependencies": {
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.7",
		"@types/nprogress": "^0.2.0",
		"autoprefixer": "^10.4.13",
		"next-sitemap": "^3.1.30",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"tailwindcss": "^3.2.1"
	}
}
